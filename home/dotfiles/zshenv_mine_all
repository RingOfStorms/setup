# BINARIES
export PATH="${HOME}/.local/bin:${PATH}"
export PATH="${HOME}/.cargo/bin:${PATH}"

# ENV VARS
. ~/.config/environment/.env

if [[ -d "$HOME/.cargo" ]]; then
  . "$HOME/.cargo/env"
fi

eval "$(/home/josh/.local/bin/mise activate zsh)"
